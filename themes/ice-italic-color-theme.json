  "semanticTokenColors": {
    "variable": { "foreground": "#c5ced4" },
    "variable.readonly": { "foreground": "#12f1fd" },
    "variable.defaultLibrary": { "foreground": "#77d8f8" },
    "property": { "foreground": "#2dd2ae", "fontStyle": "italic" },
    "parameter": { "foreground": "#e5e9ec", "fontStyle": "italic" },
    "function": { "foreground": "#3df5b8", "fontStyle": "italic" },
    "function.declaration": { "foreground": "#7ece98" },
    "function.defaultLibrary": {
      "foreground": "#3df5b8",
      "fontStyle": "italic"
    },
    "function.builtin": { "foreground": "#3df5b8", "fontStyle": "italic" },
    "method": { "foreground": "#3df5b8", "fontStyle": "italic" },
    "method.declaration": { "foreground": "#7ece98" },
    "method.defaultLibrary": { "foreground": "#3df5b8", "fontStyle": "italic" },
    "method.builtin": { "foreground": "#3df5b8", "fontStyle": "italic" },
    "keyword": { "foreground": "#d1aaf8" },
    "namespace": { "foreground": "#77d8f8", "fontStyle": "italic" },
    "namespace.defaultLibrary": {
      "foreground": "#77d8f8",
      "fontStyle": "italic"
    },
    "class": { "foreground": "#77d8f8", "fontStyle": "italic" },
    "class.defaultLibrary": { "foreground": "#77d8f8", "fontStyle": "italic" },
    "class.builtin": { "foreground": "#77d8f8", "fontStyle": "italic" },
    "struct": { "foreground": "#77d8f8", "fontStyle": "italic" },
    "type": { "foreground": "#4dc3fe" },
    "type.defaultLibrary": { "foreground": "#77d8f8", "fontStyle": "italic" },
    "operator": { "foreground": "#d1aaf8" },
    "string": { "foreground": "#63e9e2" }
  },
  "tokenColors": [
    {
      "name": "variable",
      "scope": [
        "variable.graphql",
        "variable.object.property",
        "variable.other.assignment",
        "variable.other.block.crystal",
        "variable.other.block",
        "variable.other.bracket.shell",
        "variable.other.declaration",
        "variable.other.normal.shell",
        "variable.other.object.js.jsx",
        "variable.other.object.tsx",
        "variable.other.object",
        "variable.other.php",
        "variable.other.positional.shell",
        "variable.other.readwrite.instance.crystal",
        "variable.other.readwrite",
        "variable.other.ruby",
        "variable.other",
        "variable.scss"
      ],
      "settings": { "foreground": "#c5ced4" }
    },
    {
      "name": "special variables",
      "scope": [
        "variable.language.self",
        "variable.language.special.self",
        "variable.language.super",
        "variable.language.this",
        "variable.other.alias.yaml",
        "variable.other.class.php",
        "variable.other.constant",
        "variable.other.global.safer.php",
        "variable.other.readwrite.class.ruby",
        "variable.other.readwrite.instance.ruby",
        "variable.parameter.function.language.special.self",
        "variable.other.readwrite.instance.crystal",
        "variable.other.readwrite.module"
      ],
      "settings": { "foreground": "#12f1fd", "fontStyle": "italic" }
    },
    {
      "scope": ["entity.alias.import"],
      "settings": { "foreground": "#12f1fd" }
    },
    {
      "name": "default library",
      "scope": [
        "support.variable",
        "support.constant.color.w3c-standard-color-name",
        "support.type",
        "support.class",
        "support.other.namespace"
      ],
      "settings": { "foreground": "#77d8f8", "fontStyle": "italic" }
    },
    {
      "name": "default library functions and methods",
      "scope": [
        "meta.method.declaration.js storage.type.js",
        "support.function"
      ],
      "settings": { "foreground": "#3df5b8", "fontStyle": "italic" }
    },
    {
      "name": "undecided",
      "scope": [
        "meta.generic-where-clause.swift",
        "meta.arguments.attribute.swift",
        "storage.type.annotation",
        "meta.attribute.rust"
      ],
      "settings": { "foreground": "#a8a8ff" }
    },
    {
      "name": "non variable constant",
      "scope": ["constant", "support.constant", "keyword.other.unit"],
      "settings": { "foreground": "#a8a8ff" }
    },
    {
      "name": "property, key",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name.json",
        "variable.other.property.js.jsx",
        "variable.other.property.tsx",
        "variable.other.object.property",
        "variable.other.property",
        "support.type.property-name"
      ],
      "settings": { "foreground": "#2dd2ae", "fontStyle": "italic" }
    },
    {
      "name": "parameter",
      "scope": ["meta.function-call.arguments", "variable.parameter"],
      "settings": { "foreground": "#e5e9ec", "fontStyle": "italic" }
    },
    {
      "name": "function, method",
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#7ece98" }
    },
    {
      "name": "function call",
      "scope": [
        "entity.name.function-call",
        "entity.name.function.call",
        "meta.function-call",
        "meta.method-call entity.name.function",
        "meta.function-call entity.name.function",
        "meta.function.call entity.name.function"
      ],
      "settings": { "foreground": "#3df5b8", "fontStyle": "italic" }
    },
    {
      "name": "keyword",
      "scope": [
        "keyword.control.module",
        "keyword.control",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.other.special-method",
        "keyword.type.go",
        "keyword",
        "keyword storage.type",
        "storage.modifier",
        "storage.type.class",
        "storage.type.function"
      ],
      "settings": { "foreground": "#d1aaf8" }
    },
    {
      "name": "namespace, type, struct, class, module",
      "scope": [
        "entity.name.package",
        "entity.name.type",
        "entity.name.class",
        "entity.name.namespace",
        "entity.name.module",
        "entity.name.scope-resolution",
        "storage.modifier.package.java",
        "storage.type.class.po",
        "meta.inheritance-clause",
        "entity.other.inherited-class"
      ],
      "settings": { "foreground": "#77d8f8", "fontStyle": "italic" }
    },
    {
      "name": "logical and comparison operators",
      "scope": ["keyword.operator.comparison", "keyword.operator.logical"],
      "settings": { "foreground": "#b3f2ff" }
    },
    {
      "name": "types",
      "scope": ["storage.type", "keyword.type"],
      "settings": { "foreground": "#4dc3fe" }
    },
    {
      "name": "operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#d1aaf8" }
    },
    {
      "name": "String",
      "scope": [
        "source.elixir.embedded.source",
        "storage.modifier.import.java",
        "string.quoted.double",
        "string"
      ],
      "settings": { "foreground": "#63e9e2" }
    },
    {
      "name": "embedded punctuation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": { "foreground": "#12f1fd", "fontStyle": "italic" }
    },
    {
      "name": "punctuation",
      "scope": ["meta.brace", "punctuation"],
      "settings": { "foreground": "#aab7c0" }
    },
    {
      "name": "documentation comments",
      "scope": [
        "comment.documentation",
        "comment.block.documentation",
        "string.quoted.docstring.multi.python",
        "comment.block.documentation punctuation.definition.comment"
      ],
      "settings": { "foreground": "#aab7c0" }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "punctuation.whitespace.comment.leading.graphql"
      ],
      "settings": { "foreground": "#456174", "fontStyle": "italic" }
    },
    {
      "scope": ["comment.wildcard", "meta.jsx.children"],
      "settings": { "foreground": "#c5ced4" }
    },
    {
      "name": "l2 json",
      "scope": [
        "meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#d1aaf8", "fontStyle": "italic" }
    },
    {
      "name": "l3 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#4dc3fe", "fontStyle": "italic" }
    },
    {
      "name": "l4 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#3df5b8", "fontStyle": "italic" }
    },
    {
      "name": "l5 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#77d8f8", "fontStyle": "italic" }
    },
    {
      "name": "l6 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#3df5b8", "fontStyle": "italic" }
    },
    {
      "name": "l7 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#77d8f8", "fontStyle": "italic" }
    },
    {
      "name": "l8 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#12f1fd", "fontStyle": "italic" }
    },
    {
      "name": "l9 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#a8a8ff", "fontStyle": "italic" }
    },
    {
      "name": "l10 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#d9d326", "fontStyle": "italic" }
    },
    {
      "name": "l11 json",
      "scope": [
        "meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value meta.structure.dictionary.value support.type.property-name.json"
      ],
      "settings": { "foreground": "#2dd2ae", "fontStyle": "italic" }
    },
    {
      "scope": ["meta.tag.metadata", "markup.underline.link"],
      "settings": { "foreground": "#d1aaf8" }
    },
    {
      "scope": ["meta.tag.metadata.link", "string.other.link.title.markdown"],
      "settings": { "foreground": "#7ece98" }
    },
    { "scope": ["markup.italic"], "settings": { "foreground": "#b3f2ff" } },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": { "foreground": "#788c9a" }
    },
    {
      "scope": [
        "meta.tag.inline",
        "entity.name.tag.js.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": { "foreground": "#3df5b8" }
    },
    { "scope": ["meta.tag.object"], "settings": { "foreground": "#77d8f8" } },
    {
      "scope": [
        "meta.tag.metadata.style",
        "source.vue entity.name.tag.style.html",
        "string.other.link.description.markdown"
      ],
      "settings": { "foreground": "#77d8f8" }
    },
    {
      "scope": [
        "meta.tag.metadata.script",
        "source.vue entity.name.tag.script.html"
      ],
      "settings": { "foreground": "#d9d326" }
    },
    { "scope": ["meta.tag.custom"], "settings": { "foreground": "#a8a8ff" } },
    {
      "scope": [
        "meta.tag.structure",
        "meta.tag",
        "entity.name.tag.yaml",
        "keyword.key.toml",
        "markup.fenced_code.block",
        "markup.inline.raw.string",
        "fenced_code.block.language",
        "markup.raw.block"
      ],
      "settings": { "foreground": "#2dd2ae" }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.selector.css",
        "source.css.scss entity.name.tag.css"
      ],
      "settings": { "foreground": "#3df5b8", "fontStyle": "italic" }
    },
    { "scope": ["heading"], "settings": { "foreground": "#3df5b8" } },
    {
      "scope": [
        "invalid.illegal.tag-handle.yaml",
        "text.html.markdown entity.name.tag.html",
        "text.html.markdown punctuation.definition.tag",
        "meta.tag.other.unrecognized",
        "source.vue entity.name.tag.template.html"
      ],
      "settings": { "foreground": "#12f1fd" }
    },
    {
      "scope": ["markup.bold", "punctuation.definition.bold"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "scope": [
        "markup.italic",
        "punctuation.definition.italic",
        "markup.quote.markdown"
      ],
      "settings": { "fontStyle": "italic" }
    }
  ]
}
